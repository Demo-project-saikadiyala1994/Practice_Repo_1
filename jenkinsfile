#!/usr/bin/env groovy
properties(
[
[
$class: 'GithubProjectProperty',
displayName: '',
projectUrlStr: 'https://github.com/Demo-project-saikadiyala1994/Practice_Repo_1.git/'],
pipelineTriggers([githubPush()])
]
)
pipeline {
    agent any 
    parameters {
        string(name: 'Lastname', defaultValue: '', description: 'What is your lastname?')
        booleanParam(name: 'Jenkins Knowledge', defaultValue: true, description: ' Do you have Jenkins Knowledge?')
        choice(choices: 'US-EAST-1\nUS-WEST-2', description: 'What AWS region?', name: 'region')
    }
    stages {
        stage('Build') { 
            steps { 
                echo 'last name is ${Lastname}' 
                echo 'Do you have Jenkins knowledge? ${Jenkins Knowledge}
                echo 'AWS region is ${region}
            }
        }
        stage('Test'){
            steps {
                echo 'make check'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'make publish'
            }
        }
    }
}
